Proceso Ejercicio_7
	//Declaración de variables
	//Entrada
	Definir distancia, tiempo, velocidad Como Real;
	
	Escribir "Ingresar la distancia recorrida (en Kilometros):";
	Leer distancia;
	Escribir "Ingrese el tiempo empleado (en horas):";
	Leer tiempo;
	velocidad <- distancia/tiempo;
	Escribir "La Velocidad calculada es:", velocidad, "Km/h";
	
	
FinProceso
